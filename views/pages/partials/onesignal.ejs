<script src="https://cdn.onesignal.com/sdks/OneSignalSDK.js" async></script>
<script>
  var OneSignal = OneSignal || [];
  OneSignal.push(function() {
    OneSignal.init({
      appId: "<%= oneSignal.oneSignalAppId %>",
      safari_web_id: oneSignal.oneSignalSafariId,
      autoRegister: true,
      notifyButton: {
        enable: true,
      },
    });
  });
  OneSignal.push(function() {
    OneSignal.sendTag("<%= store.getState().qt.config['publisher-name'] %>-breaking-news", "true").then(function(tagsSent) {
      console.info("Onesignal tags sent --> ", tagsSent);
    });
  });
</script>
